@using WelspunVessel.DTO.Masters
@model CountryMasterDTO
@using WelspunVessel.DTO
@{
    ViewBag.Title = " Country Detail";
}
<section class="content-header">
    <div class="col-lg-12 col-sm-12 col-md-12">
        <h2 class="main-title">View Country Detail </h2>
        <ol class="breadcrumb">
            <li><a href="@Url.Action("Index", "Home")"><i class="fa fa-home"></i> Home</a></li>
            <li><a href="@Url.Action("Index", "CountryMaster")"> Country Master</a></li>
            <li class="active">View Country Master Detail</li>
        </ol>
       
    </div>
</section>
@*<section class="content-header">
    <div class="col-lg-12 col-sm-12 col-md-12">
    <h2 class="main-title">View Country Detail</h2>
    </div>
    </section>*@
<section class="content">
    <div class="col-lg-12">
        <div class="col-lg-12 col-md-12 col-sm-12 table-layer">
            <div class="row">
                <div class="col-md-12">
                    <div class="form-group">
                        <div class="form-horizontal">
                            <fieldset class="col-md-12 fieldsetGroup">
                                <div class="col-md-6">
                                    <label asp-for="CountryName" class="control-label">Country Name</label><span class="control-label requiredStar">*</span>
                                    <input asp-for="CountryName" class="form-control" type="text" tabindex="3" autocomplete="off" disabled="disabled" />
                                    <span asp-validation-for="CountryName" class="text-danger"></span>
                                </div>
                                <div class="col-md-6">
                                    <label asp-for="IsActive" class="control-label">Status</label><span class="control-label"></span>
                                    <br />
                                    <select asp-for="IsActive" class="form-control" tabindex="16" disabled>
                                        <option value='true'>Active</option>
                                        <option value='false'>InActive</option>
                                    </select>
                                </div>
                            </fieldset>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-12" style="margin-top: 10px;">
                <input asp-for="CountryId" type="hidden" />
                <a href="@Url.Action("Index")" class="btn btn-default custom-btn" tabindex="9">Back</a>
            </div>
        </div>
    </div>
</section>