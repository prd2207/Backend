@using WelspunVessel.DTO.Masters
@model CompanyMasterDTO
@using WelspunVessel.DTO



@{
    ViewBag.Title = " Company Detail";
}



<section class="content">
    <div class="col-lg-12 col-sm-12 col-md-12">



        <h2 class="main-title">View Company Detail </h2>



        <ol class="breadcrumb">
            <li><a href="@Url.Action("Index", "Home")"><i class="fa fa-home"></i> Home</a></li>
            <li><a href="@Url.Action("Index", "CompanyMaster")"> Company Master</a></li>
            <li class="active">View Company Master Detail</li>
        </ol>
    </div>
    <div class="col-lg-12">
        <div class="col-lg-12 col-md-12 col-sm-12 table-layer">
            <div class="row">
                <div class="col-md-12">
                    <div class="form-group">
                        <div class="form-horizontal">
                            <fieldset class="col-md-12 fieldsetGroup">
                                <div class="row">
                                    <div class="col-md-6">
                                        <label asp-for="CompanyName" class="control-label"></label><span class="control-label requiredStar">*</span>
                                        <input asp-for="CompanyName" class="form-control" tabindex="2" type="text" disabled="disabled" />
                                        <span asp-validation-for="CompanyName" class="text-danger"></span>
                                    </div>
                                    <div class="col-md-6">
                                        <label asp-for="CompanySpokesperson" class="control-label"></label><span class="control-label requiredStar">*</span>
                                        <input asp-for="CompanySpokesperson" class="form-control" type="text" tabindex="3" disabled="disabled" />
                                        <span asp-validation-for="CompanySpokesperson" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label asp-for="CompanyCode" class="control-label"></label><span class="control-label requiredStar">*</span>
                                        <input asp-for="CompanyCode" class="form-control" type="text" tabindex="3" disabled="disabled" />
                                        <span asp-validation-for="CompanyCode" class="text-danger"></span>
                                    </div>
                                    <div class="col-md-6">
                                        <label asp-for="IsActive" class="control-label"></label><span class="control-label"></span>
                                        <br />
                                        <select asp-for="IsActive" class="form-control" tabindex="16" disabled>
                                            <option value='true'>Active</option>
                                            <option value='false'>InActive</option>
                                        </select>
                                    </div>
                                </div>
                            </fieldset>
                        </div>
                    </div>
                </div>
            </div>
            <div class="">
                <input asp-for="CompanyId" type="hidden" />
                <a href="@Url.Action("Index")" class="btn btn-default custom-btn" tabindex="9">Back</a>
            </div>
        </div>
    </div>
</section>